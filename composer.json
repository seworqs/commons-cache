{
  "name": "seworqs/commons-cache",
  "description": "A lightweight PSR-16 cache manager with multi-namespace support, configurable Symfony adapters, and TTL handling.",
  "type": "library",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "keywords": [
    "cache",
    "psr-16",
    "symfony",
    "ttl",
    "namespace",
    "laminas",
    "seworqs"
  ],
  "authors": [
    {
      "name": "S.E. Franke",
      "email": "sven.franke@seworqs.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seworqs\\Commons\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seworqs\\Commons\\Cache\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "psr/simple-cache": "^3.0",
    "symfony/cache": "^6.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "phpunit"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "For integration with Laminas via CacheManagerFactory"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.0.x-dev"
    }
  }
}
