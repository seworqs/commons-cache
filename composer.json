{
  "name": "seworqs/commons-cache",
  "description": "A lightweight PSR-16 cache manager with multi-namespace support, configurable adapters, and TTL handling.",
  "type": "library",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "keywords": [
    "laminas",
    "cache",
    "seworqs"
  ],
  "authors": [
    {
      "name": "S.E: Franke",
      "email": "sven.franke@seworqs.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seworqs\\Commons\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seworqs\\Commons\\Cache\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "laminas/laminas-cache": "^4.1",
    "laminas/laminas-cache-storage-adapter-memory": "^3.1"

  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "phpunit"
  }
}
